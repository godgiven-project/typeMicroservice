{
  "name": "@godgiven/microservice",
  "description": "Base Typeserver Microservice.",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "b": "yarn build",
    "w": "yarn watch",
    "s": "yarn serve",
    "c": "yarn clean",
    "cb": "yarn clean-build",
    "p": "npm-run-all --sequential prepare-publish ver release",
    "pp": "npm-run-all --sequential prepare-publish ver-patch release",
    "up": "yarn upgrade-interactive --latest && git status && bash -c 'read -p \"Continue ?\" -n 1'",
    "build": "npm-run-all lint build:*",
    "build:ts": "tsc --build",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint src/**/*.ts **/*.ts",
    "prepare": "yarn build",
    "serve": "node ./dist/server.js",
    "watch": "npm-run-all --parallel lint watch:*",
    "watch:ts": "yarn build:ts --watch",
    "watch:serve": "nodemon --watch dist ./dist/server.js",
    "clean": "tsc --build --clean",
    "clean-build": "npm-run-all --sequential clean build",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "@godgiven/type-server": "^0.1.3",
    "debug": "^4.3.3",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^38.0.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-wc": "^1.3.2",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.2"
  },
  "author": "Godgiven",
  "license": "Apache-2.0"
}
